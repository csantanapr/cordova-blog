    <plugin href="phonegap_storage_storage.md.html#name">name="<a href="phonegap_accelerometer_accelerometer.md.html#Accelerometer">Accelerometer</a>"  value="org.apache.cordova.accelerometer.<a href="phonegap_accelerometer_accelerometer.md.html#Accelerometer">Accelerometer</a>"/&gt;
