    // Cordova 準備完了
    //
    function onDeviceReady() {
        // イベントリスナーを登録
        document.addEventListener("<a href="cordova_events_events.md.html#volumeupbutton">volumeupbutton</a>", onVolumeUpKeyDown, false);
    }

    // ボリュームアップボタン関する操作を記述
    //
    function onVolumeUpKeyDown() {
    }

    &lt;/script&gt;
  &lt;/head&gt;
  &lt;body onload="onLoad()"&gt;
